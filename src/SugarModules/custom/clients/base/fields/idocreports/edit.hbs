<select class="select2 inherit-width" name="{{name}}" {{#if def.tabindex}} tabindex="{{def.tabindex}}"{{/if}} data-placholder="Select Parent Module">
	<option value=""></option>
	{{#each reports}}
	<option value="{{key}}" {{#eq key ../value}}selected="selected"{{/eq}}>{{value}}</option>
	{{/each}}
</select>
{{#unless hideHelp}}{{#if def.help}}<p class="help-block">{{str def.help module}}</p>{{/if}}{{/unless}}

